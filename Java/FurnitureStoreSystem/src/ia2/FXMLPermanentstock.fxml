<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ia2.FXMLPermanentstockController">
   <children>
      <Text layoutX="177.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Permanent Stock">
         <font>
            <Font size="33.0" />
         </font>
      </Text>
      <TableView fx:id="TblStock" layoutX="335.0" layoutY="88.0" prefHeight="200.0" prefWidth="232.0">
        <columns>
          <TableColumn fx:id="ColName" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="ColStock" prefWidth="75.0" text="Stock" />
            <TableColumn fx:id="ColDanger" text="Danger Stock" />
        </columns>
      </TableView>
      <TextField fx:id="TBName" layoutX="103.0" layoutY="100.0" promptText="Stock Name" />
      <TextField fx:id="TBStock" layoutX="103.0" layoutY="144.0" promptText="Max stock" />
      <TextField fx:id="TBDanger" layoutX="103.0" layoutY="188.0" promptText="Danger stock" />
      <Button fx:id="BTPermanentcreate" layoutX="103.0" layoutY="231.0" mnemonicParsing="false" onAction="#CreatePermanent" text="Create" />
      <Button fx:id="BTPermanentdelete" layoutX="200.0" layoutY="231.0" mnemonicParsing="false" onAction="#DeletePermanent" text="Delete" />
      <Button fx:id="BTLoad" layoutX="430.0" layoutY="299.0" mnemonicParsing="false" onAction="#getAllstudentInfo" text="Load" />
      <Button fx:id="BTBack" layoutX="275.0" layoutY="361.0" mnemonicParsing="false" onAction="#LoadBack" text="Back" />
   </children>
</AnchorPane>
